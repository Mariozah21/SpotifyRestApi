<!DOCTYPE HTML>
<html>
<table>
    <tr>
        <th> Name </th>
        <th> Album name </th>
        <th> Track length </th>
        <th> Popularity </th>
        <th> Track id </th>
    </tr>
    {% for track in tracks %}
    <tr>
        <th></th>
        <td>{{track.name}}</td>
        <td>{{track.album}}</td>
        <td>{{track.length}}</td>
        <td>{{track.popularity}}</td>
        <td>{{track.id}}</td>
    </tr>
    {% endfor %}
</table>
</html>